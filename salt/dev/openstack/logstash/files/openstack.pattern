# glusterfs

GLUSTERFS_LOGLEVEL [M|A|C|E|W|N|I|D|T]
GLUSTERFS_LOG \[%{TIMESTAMP_ISO8601:timestamp}\] %{GLUSTERFS_LOGLEVEL:glusterfs_level} \[%{DATA:file}:%{NONNEGINT}:%{DATA:function}\]

# libvirt

LIBVIRT_LOG %{TIMESTAMP_ISO8601:timestamp} %{NONNEGINT} %{WORD:level} %{DATA:functionname} \[
OPENSTACK_LOGLEVEL ([D|d]ebug|DEBUG|[N|n]otice|NOTICE|[I|i]nfo|INFO|[W|w]arn?(?:ing)?|WARN?(?:ING)?|[E|e]rr?(?:or)?|ERR?(?:OR)?|[C|c]rit?(?:ical)?|CRIT?(?:ICAL)?|[F|f]atal|FATAL|[S|s]evere|SEVERE|[A|a]udit|AUDIT|[T|t]race|TRACE)

				
# opensatck				
OPENSTACK_PROG (?:[ a-zA-Z0-9_\-]+\.)+[ A-Za-z0-9_\-$]+\b
OPENSTACK_PROG_SINGLE [A-Za-z0-9_\-$]+\b
OPENSTACK_SOURCE %{OPENSTACK_PROG}|%{OPENSTACK_PROG_SINGLE}
OPENSTACK_REQ_LIST \[((?:req-%{UUID}( None)?|%{UUID}|%{BASE16NUM}|None|-))+\]
OPENSTACK_LOG %{TIMESTAMP_ISO8601:timestamp} (%{POSINT})? %{OPENSTACK_LOGLEVEL:level} %{OPENSTACK_SOURCE:program} (%{OPENSTACK_REQ_LIST})?%{GREEDYDATA}

# openvswith
OPENVSWITCH_NUMBER [0-9]{5}
OPENVSWITCH_LOG %{TIMESTAMP_ISO8601:timestamp}\|%{OPENVSWITCH_NUMBER}\|%{DATA}\|%{WORD:openvswicth_level}


# httpd
HTTPD_ERROR_LOG1 \[%{DAY} %{MONTH:mouth} %{MONTHDAY:day} %{TIME:time} %{YEAR:year}\] \[%{WORD:module}:%{WORD:httpd_level}\] \[pid %{NONNEGINT}\] %{WORD}: %{DATA}
HTTPD_ERROR_LOG2 \[%{DAY} %{MONTH:mouth} %{MONTHDAY:day} %{TIME:time} %{YEAR:year}\] \[:%{WORD:httpd_level}\] \[pid %{NONNEGINT}\]
HTTPD_ERROR_LOG (?:%{HTTPD_ERROR_LOG1}|%{HTTPD_ERROR_LOG2})


HTTPD_ACCESS_LOG (?:%{COMBINEDAPACHELOG}|%{COMMONAPACHELOG})


